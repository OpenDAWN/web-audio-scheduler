{
  "name": "web-audio-scheduler",
  "description": "Event Scheduler for Web Audio API",
  "version": "0.1.0",
  "author": "mohayonao <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/web-audio-scheduler/issues"
  },
  "devDependencies": {
    "6to5": "^2.12.0",
    "browserify": "^8.1.1",
    "espower-6to5": "^1.1.0",
    "istanbul": "^0.3.5",
    "jshint": "^2.5.11",
    "jshint-stylish": "^1.0.0",
    "mocha": "^2.1.0",
    "power-assert": "^0.10.1",
    "tickable-timer": "0.0.1",
    "uglify-js": "^2.4.16",
    "web-audio-test-api": "^0.1.16"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/mohayonao/web-audio-scheduler/",
  "keywords": [
    "webaudioapi",
    "scheduler",
    "event"
  ],
  "license": "MIT",
  "main": "lib/web-audio-scheduler.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohayonao/web-audio-scheduler.git"
  },
  "scripts": {
    "build": "mkdir -p build && npm run build-6to5 && npm run build-browser && npm run build-minify",
    "build-6to5": "6to5 src --out-dir lib --source-maps-inline",
    "build-browser": "browserify lib/web-audio-scheduler.js --standalone WebAudioScheduler > build/web-audio-scheduler.js",
    "build-minify": "uglifyjs build/web-audio-scheduler.js > build/web-audio-scheduler.min.js",
    "cover": "npm run build-6to5 && istanbul cover node_modules/mocha/bin/_mocha",
    "lint": "jshint --reporter node_modules/jshint-stylish/stylish.js src",
    "test": "npm run build-6to5 && mocha test/*.js"
  }
}
